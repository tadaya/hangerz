<!DOCTYPE html>
<html>
<head>
  <title>Hangerz</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
  <% if session[:user_id] %>
    <ul class="nav">    
      <li> <%= link_to("Log Out #{current_user.first_name}", "/logout", method: "delete") %> </li>
    </ul>
  <% end %>
</header>

<%= yield %>

<%= debug session.to_hash %>
<%= debug current_user %>
<%= debug params %>
</body>
</html>
